{
  "name": "bestbets-loader",
  "version": "2.0.0",
  "description": "Loader for BestBets API",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --collectCoverageFrom=lib/**.js --coverage lib"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "<rootDir>/lib/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/.*\\.json/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nciocpl/bestbets-loader.git"
  },
  "author": "Bryan Pizzillo",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/nciocpl/bestbets-loader/issues"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "homepage": "https://github.com/nciocpl/bestbets-loader#readme",
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.0",
    "@octokit/rest": "^19.0.4",
    "agentkeepalive": "^4.2.1",
    "axios": "^0.27.2",
    "config": "^3.3.7",
    "elastic-tools": "github:nciocpl/elastic-tools#v3.0.0",
    "html-entities": "^2.3.3",
    "loader-pipeline": "github:nciocpl/loader-pipeline#v1.1.0",
    "p-limit": "^2.3.0",
    "parse-github-repo-url": "^1.4.1",
    "winston": "^3.8.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.56",
    "jest": "^29.0.1",
    "nock": "^13.5.4",
    "winston-null-transport": "github:NCIOCPL/winston-null-transport#1.0.0"
  }
}
